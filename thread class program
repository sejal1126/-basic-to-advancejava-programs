class Main extends Thread{
  public void run()
    {
    int i=0;
    while(i<10)
    {
      System.out.println(i+"hello world!!");
      i++;
    }
    }
  
  public static void main(String args[])
  {
    int i=0;
    Main t = new Main();
    t.start();
    i=0;
    while(i<10)
    {
      System.out.println(i+"hello sejal!!");
      i++;
    }
  }
}
